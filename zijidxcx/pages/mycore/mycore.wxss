/* pages/mycore/mycore.wxss */
page{
  background: #f1f1f1;
}
.bg{
  width: 100%;
  height: 250rpx;
  background: lightblue;
  opacity: .9;
  position: relative;
}
.headImg{
  width: 100rpx;
  height: 100rpx;
  border: 1rpx solid red;
  position: absolute;
  top: 50%;
  left: 80rpx;
  margin-top: -50rpx;
  border-radius: 50%;
  overflow: hidden;
}
.myImg{
  width: 100%;
  height: 100%;
}
.myInfo{
  position: absolute;
  right: 50rpx;
  top: 50%;
  margin-top: -7.5rpx;
}
.order{
  margin-top: 20rpx;
  width: 100%;
  padding: 10rpx 0;
  background: #fff;
}
.divider{
  display: block;
  padding:20rpx 0 20rpx 30rpx; 
  font-size: 14px;
}
.iconfont{
  font-size: 30px;
}
.nav-item{
  height: 140rpx;
}