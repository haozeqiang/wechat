/* 引入公共模板的样式 */
@import "/pages/complete/complete";
/**app.wxss**/
page{
  background: #f1f1f1;
}
/* div下边线 */
.divider{
  border-bottom: 1rpx solid #ddd;
}
/* 通用箭头样式 */
.arrow{
  display: flex;
  justify-content: space-between;/* 两端对齐 */
  align-items: center;/* 垂直居中 */
}
.arrow::after{
  content:'';
  width: 15rpx;
  height: 15rpx;
  border-top: 3rpx solid #000;
  border-right: 3rpx solid #000;
  transform: rotate(45deg);
  opacity: .8;
}
/* 轮播图开始 */
swiper{
  height: 390rpx;
}
swiper image{
  width: 100%;
  height: 100%;
}
/* 轮播图结束 */
.title{
  font-size: 16px;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.price{
  color:red;
  font-size: 14px;
}
/* 单个商品详细信息 */
.prolist{
  position: relative;
}
.prolist .ldiv{
  width: 15rpx;
  height: 45rpx;
  margin: auto auto;
  background: rgb(78, 177, 235);
  position: absolute;
  top: 10rpx;

}
.prolist .ftitle{
  background: rgba(223, 219, 219, 0.829);
}
.prolist .ftitle text{
  font-size: 15px;
  margin-left: 30rpx;
  line-height: 2.2;
}
.proinfo{
  position: relative;
  display: flex;
  border-bottom:1rpx solid #ddd;
}
.shopinfo{
  width: 70%;
  position: absolute;
  top: 0;
  right: 0;
}
.proinfo image{
  width: 30%;
  height: 200rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.shoptitle .title,.shopprice .price{
  line-height: 2;
}
.shopbottom{
  padding-top: 10rpx;
  display: flex;
  justify-content: space-between;
}
.shopbottom text{
  align-items: bottom;
}
.shopbottom .iconfont{
  font-size: 29px;
}
.shopbottom .shopcount{
  font-size: 14px;
  vertical-align: bottom;
  margin-top: 20rpx;
}
/* 公用样式值文本超出范围隐藏不换行 */
.ell{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/* 引入阿里巴巴矢量图 */
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

